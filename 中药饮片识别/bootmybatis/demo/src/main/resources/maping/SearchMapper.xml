<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.peng.mapper.SearchMapper">

    <select id="search" resultType="Search">
        select *
        from search
        where name = #{searchName}
    </select>
</mapper>
