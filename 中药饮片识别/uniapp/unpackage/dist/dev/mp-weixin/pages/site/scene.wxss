.neirong {
  width: 100%;
  position: relative;
  background-color: #FFF;
  min-height: 200px;
}
.neirong .contbox {
  font-size: 56rpx;
  color: #333;
  box-sizing: border-box;
  max-height: 9999rpx;
}
.neirong .contbox .name .font {
  font-weight: bold;
  font-size: 55rpx;
}
.neirong .contbox .content {
  background-color: #f8fcff;
  border: solid 1px #dbeef;
  padding-bottom: 10rpx;
  position: relative;
  margin-top: 10px;
  font-size: 28rpx;
}
.neirong .contbox .content .cont {
  max-height: none;
  overflow: hidden;
  color: #666;
  line-height: 1.8em;
  box-sizing: border-box;
  font-size: 28rpx;
}
.neirong .contbox .middle .lijian {
  width: 100%;
  box-sizing: border-box;
  line-height: 1.8em;
  max-height: 9999999px;
  padding: 0px;
  margin: 0px;
  border: 0px;
}
.neirong .contbox .middle .lijian .dhidden {
  line-height: 1.8em;
  border-bottom: dashed   1px #CCC;
  padding: 8px 0px;
  position: relative;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  display: block;
  list-style: none;
  font-size: 28rpx;
}
.neirong .contbox .middle .lijian .dhidden .fcolor {
  color: #47a0e5;
  font-style: normal;
  box-sizing: border-box;
}
.neirong .contbox .title {
  text-align: center;
  font-size: 28rpx;
  margin: 50rpx 0rpx 20rpx;
}
.neirong .contbox .title .font1 {
  font-size: 40rpx;
}
.neirong .contbox .txt {
  max-height: 100em;
  overflow: hidden;
  color: #666;
  line-height: 1.8em;
  font-size: 28rpx;
}
.neirong .contbox .map {
  margin: 50rpx 0rpx 20rpx;
  text-align: center;
  font-size: 28rpx;
}
.neirong .contbox .map .font2 {
  font-size: 40rpx;
}
.neirong .contbox .allmap {
  width: 100%;
  height: 800rpx;
  border: solid 1px #8dbde6;
  overflow: hidden;
  position: relative;
  text-align: left;
  background-color: white;
  color: black;
}
.neirong .contbox .allmap .Map_msk {
  position: absolute;
  left: 0px;
  top: 0px;
  z-index: 9;
  overflow: hidden;
  width: 750rpx;
  height: 800rpx;
}
.txt ._p {
  padding-bottom: 25px;
}

